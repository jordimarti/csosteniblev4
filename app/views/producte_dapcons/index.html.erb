<p id="notice"><%= notice %></p>

<h1>Producte Dapcons</h1>

<table>
  <thead>
    <tr>
      <th>Nom ca</th>
      <th>Nom es</th>
      <th>Nom en</th>
      <th>Descripcio ca</th>
      <th>Descripcio es</th>
      <th>Descripcio en</th>
      <th>Identificador</th>
      <th>Rcp ca</th>
      <th>Rcp es</th>
      <th>Rcp en</th>
      <th>Validesa inici</th>
      <th>Validesa fi</th>
      <th>Empresa</th>
      <th>Categoria indicadors</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @producte_dapcons.each do |producte_dapcons| %>
      <tr>
        <td><%= producte_dapcons.nom_ca %></td>
        <td><%= producte_dapcons.nom_es %></td>
        <td><%= producte_dapcons.nom_en %></td>
        <td><%= producte_dapcons.descripcio_ca %></td>
        <td><%= producte_dapcons.descripcio_es %></td>
        <td><%= producte_dapcons.descripcio_en %></td>
        <td><%= producte_dapcons.identificador %></td>
        <td><%= producte_dapcons.rcp_ca %></td>
        <td><%= producte_dapcons.rcp_es %></td>
        <td><%= producte_dapcons.rcp_en %></td>
        <td><%= producte_dapcons.validesa_inici %></td>
        <td><%= producte_dapcons.validesa_fi %></td>
        <td><%= producte_dapcons.empresa %></td>
        <td><%= producte_dapcons.categoria_indicadors %></td>
        <td><%= link_to 'Show', producte_dapcons %></td>
        <td><%= link_to 'Edit', edit_producte_dapcons_path(producte_dapcons) %></td>
        <td><%= link_to 'Destroy', producte_dapcons, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

