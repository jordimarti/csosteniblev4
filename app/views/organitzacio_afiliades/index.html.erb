<p id="notice"><%= notice %></p>

<h1>Organitzacio Afiliades</h1>

<table>
  <thead>
    <tr>
      <th>Numero membre</th>
      <th>Nom</th>
      <th>Dades</th>
      <th>Link iso</th>
      <th>Tipus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organitzacio_afiliades.each do |organitzacio_afiliada| %>
      <tr>
        <td><%= organitzacio_afiliada.numero_membre %></td>
        <td><%= organitzacio_afiliada.nom %></td>
        <td><%= organitzacio_afiliada.dades %></td>
        <td><%= organitzacio_afiliada.link_iso %></td>
        <td><%= organitzacio_afiliada.tipus %></td>
        <td><%= link_to 'Show', organitzacio_afiliada %></td>
        <td><%= link_to 'Edit', edit_organitzacio_afiliada_path(organitzacio_afiliada) %></td>
        <td><%= link_to 'Destroy', organitzacio_afiliada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organitzacio Afiliada', new_organitzacio_afiliada_path %>
